{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "quotes": ["error", "single"],
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "max-len": ["warn", {"code": 150}],
        "no-param-reassign": ["off"],
        "no-console": ["off"],
        "import/prefer-default-export": ["off"],
        "import/extensions": ["off"],
        "import/no-unresolved": ["off"]
    }
}